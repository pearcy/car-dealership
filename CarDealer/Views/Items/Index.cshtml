@using CarDealer.Models;

<h1>Larry's Lemons</h1>

@if (Model.Count == 0)
{
  <p>There are no items in the list.</p>
}
<ul>
  @foreach (Item auto in Model) 
  {
    <li>@auto.Description</li>
    <li>@auto.Miles</li>
    <li>@auto.Accident</li>
    <li>@auto.Price</li>
  }

</ul>
<a href="/items/new">Add a new item.</a>